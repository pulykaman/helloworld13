class Clones
{
   public static void main(String args[])
   {
      addClones(4, 5);
      addClones2(4, 5);
   }
   
   public static void addClones(int x, int y){
      int z;
 
      x = in.nextInt();
      y = in.nextInt();
      z = x + y;
 
      System.out.println("Sum of the clones = " + z);
   }
   
   public static void addClones2(int x, int y){
      int z;
 
      x = in.nextInt();
      y = in.nextInt();
      z = x + y;
 
      System.out.println("Sum of the clones = " + z);
   }
   
}
